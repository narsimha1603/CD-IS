#include <stdio.h>
#include <ctype.h>
#include <string.h>
char buf[512];
char *p;
void skipws(){ while(*p && isspace((unsigned char)*p)) p++; }
int is_id_start(char c){ return isalpha((unsigned char)c) || c=='_'; }
int is_id_char(char c){ return isalnum((unsigned char)c) || c=='_'; }
int parseT(void){
    skipws();
    if (!is_id_start(*p)) return 0;
    p++;
    while(is_id_char(*p)) p++;
    skipws();
    return 1;
}
int parseE(void){
    if (!parseT()) return 0;
    while(1){
        skipws();
        if (*p == '+'){
            p++; /* consume + */
            if (!parseT()) return 0; /* require T after + */
        } else break;
    }
    return 1;
}
int main(void){
    while (1){
        if (!fgets(buf, sizeof(buf), stdin)) break;
        size_t n = strlen(buf);
        if (n && buf[n-1]=='\n') buf[n-1]=0;
        if (buf[0]==0) break; /* empty line -> exit */
        p = buf;
        skipws();
        if (parseE() && (*p == '\0')) puts("ok");
        else puts("error");
    }
    return 0;
}
